{
  "name": "@cultural-archiver/mass-import",
  "version": "1.0.0",
  "description": "Mass import library for Cultural Archiver - automated ingestion of public art data",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "mass-import": "./dist/cli/index.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./cli": {
      "import": "./dist/cli.js",
      "types": "./dist/cli.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest",
    "test:basic": "tsx test/test-basic.ts",
    "test:integration": "npm run build && tsx test/test-integration.ts",
    "lint": "eslint src/",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "chalk": "^5.3.0",
    "commander": "^11.0.0",
    "fuzzy": "^0.1.3",
    "ora": "^7.0.0",
    "progress": "^2.0.3",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/progress": "^2.0.7",
    "eslint": "^8.57.0",
    "tsx": "^4.20.5",
    "typescript": "^5.6.0",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "cultural-archiver",
    "mass-import",
    "public-art",
    "data-import"
  ],
  "author": "Steven Smethurst",
  "license": "MIT"
}
