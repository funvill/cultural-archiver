    "leaflet": "^1.9.4",
    "pinia": "^2.2.0",
    "vue": "^3.5.0",
    "vue-router": "^4.4.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@vitejs/plugin-vue": "^5.0.0",
    "@vitest/ui": "^3.2.4",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.5.0",
    "autoprefixer": "^10.4.0",
    "jsdom": "^26.1.0",
    "postcss": "^8.4.0",
    "tailwindcss": "^3.4.0",
