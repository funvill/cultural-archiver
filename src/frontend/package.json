{
  "name": "@cultural-archiver/frontend",
  "version": "1.0.0",
  "description": "Frontend application for Cultural Archiver",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
  "prebuild:manifest": "node scripts/generate-pages-manifest.cjs",
  "build": "npm run prebuild:manifest && vue-tsc --noEmit && vite build",
    "deploy": "npm run build && wrangler deploy",
    "preview": "vite preview",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest run",
    "test:e2e": "playwright test",
    "test:watch": "vitest",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "sync-docs": "node scripts/sync-docs.js"
  },
  "dependencies": {
    "@deck.gl/core": "^9.0.0",
    "@deck.gl/layers": "^9.0.0",
    "@headlessui/vue": "^1.7.0",
    "@heroicons/vue": "^2.0.0",
    "@loaders.gl/images": "^4.0.0",
    "@types/leaflet": "^1.9.20",
    "@vue-leaflet/vue-leaflet": "^0.10.1",
    "@vueuse/core": "^11.0.0",
    "axios": "^1.7.0",
    "deck.gl": "^9.0.0",
    "dompurify": "^3.0.3",
    "exifr": "^7.1.3",
    "isomorphic-dompurify": "^2.28.0",
    "leaflet": "^1.9.4",
    "marked": "^12.0.2",
    "pinia": "^2.2.0",
    "supercluster": "^8.0.1",
    "vue": "^3.5.0",
    "vue-router": "^4.4.0"
  },
  "devDependencies": {
  "@playwright/test": "^1.55.0",
    "@types/marked": "^5.0.2",
    "@types/node": "^22.0.0",
    "@types/supercluster": "^7.1.3",
    "@vitejs/plugin-vue": "^5.0.0",
    "@vitest/ui": "^3.2.4",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.5.0",
    "autoprefixer": "^10.4.0",
    "install": "^0.13.0",
    "jsdom": "^26.1.0",
    "npx": "^10.2.2",
    "playwright": "^1.55.0",
    "postcss": "^8.4.0",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.6.0",
    "vite": "^5.4.0",
    "vitest": "^3.2.4",
    "vue-tsc": "^2.1.0",
    "gray-matter": "^4.0.3"
  }
