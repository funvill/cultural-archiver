{
  "defaultConfig": {
    "user_uuid": "a0000000-1000-4000-8000-000000000002",
    "duplicateThreshold": 0.7,
    "duplicateDetectionRadius": 10000,
    "attribution": {
      "source": "OpenStreetMap",
      "license": "[ODbL](https://www.openstreetmap.org/copyright)",
      "attribution_text": "© OpenStreetMap contributors"
    },
    "fieldMappings": {
      "title": "name",
      "artist": "artist_name",
      "description": "description"
    },
    "validation": {
      "requireName": true,
      "requireCoordinates": true,
      "skipIncomplete": true
    }
  },
  "presets": {
    "vancouver": {
      "description": "Configuration for Vancouver public art imports",
      "user_uuid": "a0000000-1000-4000-8000-000000000002",
      "duplicateThreshold": 0.8,
      "duplicateDetectionRadius": 10000,
      "attribution": {
        "source": "OpenStreetMap",
        "license": "[ODbL](https://www.openstreetmap.org/copyright)",
        "attribution_text": "© OpenStreetMap contributors"
      },
      "fieldMappings": {
        "title": "name",
        "artist": "artist_name",
        "description": "description"
      },
      "validation": {
        "requireName": true,
        "requireCoordinates": true,
        "skipIncomplete": true
      },
      "additionalTags": [
        { "label": "Import Source", "value": "Vancouver OSM Dataset" },
        { "label": "Data Quality", "value": "Community Verified" }
      ]
    },
    "strict": {
      "description": "Strict validation for high-quality imports",
      "user_uuid": "a0000000-1000-4000-8000-000000000002",
      "duplicateThreshold": 0.9,
      "attribution": {
        "source": "OpenStreetMap",
        "license": "[ODbL](https://www.openstreetmap.org/copyright)",
        "attribution_text": "© OpenStreetMap contributors"
      },
      "fieldMappings": {
        "title": "name",
        "artist": "artist_name",
        "description": "description"
      },
      "validation": {
        "requireName": true,
        "requireCoordinates": true,
        "requireArtist": true,
        "requireArtworkType": true,
        "skipIncomplete": true
      }
    },
    "permissive": {
      "description": "Permissive validation for maximum data inclusion",
      "user_uuid": "a0000000-1000-4000-8000-000000000002",
      "duplicateThreshold": 0.5,
      "attribution": {
        "source": "OpenStreetMap",
        "license": "[ODbL](https://www.openstreetmap.org/copyright)",
        "attribution_text": "© OpenStreetMap contributors"
      },
      "fieldMappings": {
        "title": "name",
        "artist": "artist_name",
        "description": "description"
      },
      "validation": {
        "requireName": false,
        "requireCoordinates": true,
        "skipIncomplete": false,
        "generateTitleFromLocation": true
      }
    }
  },
  "batchProcessing": {
    "defaultBatchSize": 50,
    "maxBatchSize": 200,
    "processingMode": "sequential",
    "retryFailedBatches": true,
    "maxRetries": 3,
    "retryDelayMs": 1000
  },
  "fieldMappingReference": {
    "title": "Artwork title/name - mapped from OSM 'name' tag",
    "artist": "Artist name - mapped from OSM 'artist_name' tag",
    "description": "Artwork description - mapped from OSM 'description' tag",
    "coordinates": "Latitude/longitude - extracted from GeoJSON geometry",
    "external_id": "OSM feature ID - used for duplicate detection",
    "artwork_type": "Type of artwork - mapped from OSM 'artwork_type' tag",
    "material": "Construction material - mapped from OSM 'material' tag",
    "website": "Related website URL - mapped from OSM 'website' tag"
  },
  "tagMappingRules": {
    "preserveAllOSMTags": true,
    "excludeTags": ["osm_type", "osm_id", "tourism"],
    "transformations": {
      "artwork_type": {
        "transform": "title_case",
        "label": "Artwork Type"
      },
      "material": {
        "transform": "title_case",
        "label": "Material"
      },
      "website": {
        "label": "Website",
        "validation": "url"
      },
      "inscription": {
        "label": "Inscription"
      },
      "start_date": {
        "label": "Created Date",
        "transform": "date_format"
      }
    }
  },
  "validation": {
    "coordinateValidation": {
      "minLat": -90,
      "maxLat": 90,
      "minLon": -180,
      "maxLon": 180,
      "precision": 6
    },
    "requiredFields": {
      "default": ["coordinates"],
      "strict": ["coordinates", "name", "artist_name", "artwork_type"],
      "permissive": ["coordinates"]
    }
  }
}
