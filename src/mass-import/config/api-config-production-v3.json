{
  "importer": {
    "includeUnknownFeatureTypes": true
  },
  "exporter": {
    "apiEndpoint": "https://api.publicartregistry.com/api/mass-import/v3",
    "method": "POST",
    "headers": {
      "Content-Type": "application/json"
    },
    "authentication": {
      "type": "bearer",
      "token": "a0000000-1000-4000-8000-000000000001"
    },
    "timeout": 30000,
    "retryAttempts": 3,
    "retryDelay": 1000,
    "validateResponse": true,
    "transformData": true,
    "logRequests": true,
    "logResponses": true,
    "photoCacheDir": ".cache/photos",
    "skipDuplicateDetection": false,
    "autoApproveArtists": true
  }
}
